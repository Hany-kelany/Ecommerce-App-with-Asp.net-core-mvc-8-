
@if (TempData["Created"] != null)
{
   
    <script type="text/javascript">
        toastr["success"]("@TempData["Created"]")
    </script>
 
}

@if (TempData["Updated"] != null)
{
   
        <script type="text/javascript">
        toastr["warning"]("@TempData["Updated"]");
    </script>
}

@if (TempData["Deleted"] != null)
{
 
    <script type="text/javascript">
        toastr["error"]("@TempData["Deleted"]");
    </script>
}

